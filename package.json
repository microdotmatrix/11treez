  "version": "0.7.0",
  "description": "A simple Eleventy starter kit to help you have a blog of your own",
  "main": "index.js",
  "dependencies": {
    "json-to-scss": "^1.5.0",
    "sass": "^1.26.3",
    "semver": "^6.3.0",
    "slugify": "^1.4.0",
